name = "shorter"
compatibility_date = "2024-01-01"
pages_build_output_dir = "./dist"

# KV 命名空间绑定
# 1. 创建 KV: npx wrangler kv namespace create LINKS_KV
# 2. 创建预览 KV: npx wrangler kv namespace create LINKS_KV --preview
# 3. 将生成的 id 填入下方，然后重命名此文件为 wrangler.toml
[[kv_namespaces]]
binding = "LINKS_KV"
id = "your-kv-namespace-id-here"
preview_id = "your-preview-kv-namespace-id-here"

# 用户 KV 命名空间
# 创建: npx wrangler kv namespace create USERS_KV
# 预览: npx wrangler kv namespace create USERS_KV --preview
[[kv_namespaces]]
binding = "USERS_KV"
id = "your-users-kv-namespace-id-here"
preview_id = "your-users-preview-kv-namespace-id-here"

# 环境变量
[vars]
JWT_SECRET = "your-jwt-secret-here"

