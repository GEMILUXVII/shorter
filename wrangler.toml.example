name = "shorter"
compatibility_date = "2024-01-01"
pages_build_output_dir = "./dist"

# KV 命名空间绑定
# 1. 创建 KV: npx wrangler kv namespace create LINKS_KV
# 2. 创建预览 KV: npx wrangler kv namespace create LINKS_KV --preview
# 3. 将生成的 id 填入下方，然后重命名此文件为 wrangler.toml
[[kv_namespaces]]
binding = "LINKS_KV"
id = "your-kv-namespace-id-here"
preview_id = "your-preview-kv-namespace-id-here"
